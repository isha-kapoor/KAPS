    <form class="new-cc" method="post" action="/newcc-username">
      <label for="cc">Name</label>
      <input type="text" id="cc" name="ccname">
      <br>
      <label for="cc-add">Address</label>
      <input type="text" id="cc-add" name="ccadd">
      <br>
      <label for="cc-contact">Contact Number</label>
      <input type="text" id="cc-contact" name="cccontact" pattern="[0-9]{10}"
						maxlength="10" >
      <br>
      <label for="cc-username">Username</label>
      <input type="text" id="cc-username" name="ccusername">
      <br>
      <label for="cc-password">Password</label>
      <input type="text" id="cc-password" name="ccpassword">
      <br>
      <label for="cc-confirm">Confirm password</label>
      <input type="text" id="cc-confirm" name="ccconfirm">
      <br>
      <label for="cc-confirm">Select</label>
      <select type="text" id="cc-confirm" name="select">
        <option value="Farmer">Famer</option>
        <option value="CollectionCentre">CollectionCentre</option>
