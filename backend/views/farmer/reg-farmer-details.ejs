    <form class="choose-cc" method="POST" action="/farmerregdetails"> <!---New Farmer Registration-->
      <label for="f-cc">Choose Collection Centre</label>
      <select class="select-cc" id="f-cc" name="fcc" required>
        <% records.forEach(function(row){%>
        <option value="<%=row.ccname%>"><%=row.ccname%></option>
        <% }) %>
        <!-- <option value="Bharwala">Bharwala</option>
        <option value="Jamnagar">Jamnagar</option>
        <option value="Ranpur">Ranpur</option> -->
      </select>
      <br>
      <br>
      <label for="f-cc">Choose Kharif Crops Grown</label>
      <select class="select-crops-k" id="select-crops" name="fKcrops" multiple required>
        <option value="Rice">Rice</option>
        <option value="Sugar Cane">Sugar Cane</option>
        <option value="Cotton">Cotton</option>
      </select>
      <br>
      <br>
      <label for="f-cc">Choose Rabi Crops Grown</label>
      <select class="select-crops-r" id="select-crops" name="fRcrops" multiple required>
        <option value="Wheat">Wheat</option>
        <option value="Sugar Cane">Sugar Cane</option>
      </select>
      <input type="submit" value="Save">
    </form>

