<script type="text/javascript">

anychart.onDocumentReady(function() {
  // create map
  const databases = '<%-jsonData%>'
  const datas = jQuery.parseJSON(databases)
  const items=[];
  const itemss=[];
  for(i=0;i<datas.length;i++)
  {
    items.push(datas[i].name)
    itemss.push(datas[i].code)
  } // state's data
  var rows =<%-JSON.stringify(details)%>;
  console.log(rows)
  var data =[];
  for(j=0;j<items.length;j++)
  {
    var flag =0;
    for(i=0;i<rows.length ;i++){
      if(items[j] == rows[i]._id){ flag=1; data.push([itemss[j] , rows[i].total])}
    }
    if(!flag) data.push([itemss[j] , 0])
 }
  var map = anychart.map();
  map.colorRange(true);
  map.title("Registered Private Companies States")
  series = map.choropleth(data);
  series.colorScale(anychart.scales.linearColor('#deebf7', '#3182bd'));
  series.hovered().fill('#addd8e');
  map.geoData(anychart.maps['india']);
  map.container('container');
  map.draw();
});
</script>
